# Arize API key for authentication
# Get this from your Arize account settings
ARIZE_API_KEY=your_api_key_here

# Arize Developer key for datasets and experiments
# Get this from your Arize account settings
ARIZE_DEVELOPER_KEY=your_developer_key_here

# Arize Space ID for datasets and experiments
# Get this from your Arize account settings
ARIZE_SPACE_ID=your_space_id_here

# Default dataset ID to use when --dataset flag is not provided
DATASET=your_default_dataset_here
